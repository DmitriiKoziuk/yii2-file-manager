{
  "name": "dmitriikoziuk/yii2-file-manager",
  "description": "yii2 file manager extension",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "file manager",
    "upload files",
    "upload images"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitrii Koziuk",
      "email": "dmitriikoziuk@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "ext-imagick": "*",
    "ext-fileinfo": "*",
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-imagine": "^2.2",
    "yiisoft/yii2-queue": "^2.3",
    "kartik-v/yii2-widget-fileinput": "^1.0",
    "dmitriikoziuk/yii2-base": "~0.3.0",
    "dmitriikoziuk/yii2-module-manager": "~0.3.0",
    "dmitriikoziuk/yii2-config-manager": "~0.3.0"
  },
  "autoload": {
    "psr-4": {
      "DmitriiKoziuk\\yii2FileManager\\": "src"
    }
  },
  "extra": {
    "bootstrap": "DmitriiKoziuk\\yii2FileManager\\Bootstrap"
  }
}
